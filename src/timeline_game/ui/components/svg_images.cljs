(ns timeline-game.ui.components.svg-images)

(defn embed [id path [x y width height]]
  [:svg
   {:style {:display :none}}
   [:symbol
    {:id id
     :viewBox (str x " " y " " width " " height)}
    [:path.path
     {:fill "none"
      :stroke "black"
      :stroke-width 1
      :d path}]]])

(defn ref [id attributes]
  [:svg
   attributes
   [:use
    {:href id}]])

(defn embed-clip [id path [width height]]
  [:svg
   {:width 0
    :height 0}
   [:defs
    [:clipPath
     {:id id
      :clipPathUnits "objectBoundingBox"
      :transform (str "scale(" (/ 1 width) (/ 1 height) ")")}
     [:path
      {:fill "none"
       :stroke "black"
       :stroke-width "1"
       :d path}]]]])

